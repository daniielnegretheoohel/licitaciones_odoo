<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_desarrollo_personal_user" model="res.groups">
        <field name="name">Usuario de Desarrollo Personal</field>
        <!-- Elimina la línea de category_id o usa una categoría existente -->
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_desarrollo_personal_manager" model="res.groups">
        <field name="name">Administrador de Desarrollo Personal</field>
        <field name="implied_ids" eval="[(4, ref('group_desarrollo_personal_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>
</odoo>