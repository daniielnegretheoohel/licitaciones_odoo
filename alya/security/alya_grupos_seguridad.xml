<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="module_category_desarrollo_personal" model="ir.module.category">
        <field name="name">Desarrollo Personal</field>
        <field name="description">Perfiles de acceso para el m√≥dulo de desarrollo personal</field>
        <field name="sequence">10</field>
    </record>

    <record id="alya_group_desarrollo_personal_user" model="res.groups">
